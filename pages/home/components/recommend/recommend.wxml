<!--pages/home/components/recomend/recommend.wxml-->
<view class="recommend">
  <block wx:for="{{recommend}}" wx:key="acm">
    <view class="recommend-item">
      <image class="recommend-item-img" src="{{item.image}}"></image>
      <view class="recommend-item-text">{{item.title}}</view>
    </view>
  </block>
</view>